{
  "name": "material-components-vue",
  "version": "1.0.2",
  "description": "Material Design components for Vue.js",
  "author": "Mats Pfeiffer <mats.pfeiffer@googlemail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matsp/material-components-vue.git"
  },
  "keywords": [
    "material-components-web",
    "mdc-web",
    "material-design",
    "vue"
  ],
  "homepage": "https://matsp.github.io/material-components-vue",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .js --ext .vue --fix ./",
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:min": "cross-env NODE_ENV=production webpack --config webpack.config.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:prod": "vuepress build docs",
    "watch:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js -w",
    "release": "npm run build && npm run build:min && standard-version",
    "test": "cross-env NODE_ENV=testing jest",
    "test:watch": "npm run test -- --watch",
    "test:updateSnapshot": "npm run test -- --updateSnapshot"
  },
  "dependencies": {
    "material-components-web": "2.2.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/preset-env": "7.4.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "2.0.2",
    "copy-webpack-plugin": "5.0.3",
    "core-js": "3.0.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "jest-serializer-vue": "2.0.2",
    "mini-css-extract-plugin": "0.6.0",
    "mutationobserver-shim": "0.3.3",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss": "7.0.16",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "standard-version": "6.0.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "1.1.2",
    "vue-jest": "3.0.4",
    "vue-loader": "15.7.0",
    "vuepress": "0.14.11",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "html",
      "jest"
    ],
    "settings": {
      "html/html-extensions": [
        ".html"
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    }
  },
  "eslintIgnore": [
    "/dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@material)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  }
}
