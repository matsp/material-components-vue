{
  "name": "material-components-vue",
  "version": "1.2.0",
  "description": "Material Design components for Vue.js",
  "author": "Mats Pfeiffer <mats.pfeiffer@googlemail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matsp/material-components-vue.git"
  },
  "keywords": [
    "material-components-web",
    "mdc-web",
    "material-design",
    "vue"
  ],
  "homepage": "https://matsp.github.io/material-components-vue",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --ext .js --ext .vue --fix ./",
    "build": "cross-env NODE_ENV=development webpack --config build/webpack.config.babel.js",
    "build:min": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.babel.js",
    "docs:dev": "vuepress dev docs",
    "docs:prod": "vuepress build docs",
    "storybook": "start-storybook",
    "watch:dev": "cross-env NODE_ENV=development webpack --config build/webpack.config.babel.js -w",
    "release": "npm run build && npm run build:min && standard-version",
    "test": "cross-env NODE_ENV=testing jest",
    "test:watch": "npm run test -- --watch",
    "test:updateSnapshot": "npm run test -- --updateSnapshot"
  },
  "dependencies": {
    "material-components-web": "5.1.0"
  },
  "peerDependencies": {
    "vue": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-knobs": "5.3.14",
    "babel-preset-vue": "2.0.2",
    "@babel/core": "7.8.6",
    "@babel/register": "7.8.6",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-object-assign": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@fortawesome/fontawesome-free": "5.12.1",
    "@storybook/vue": "5.3.14",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.4",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "dart-sass": "1.25.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-json": "2.1.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.1",
    "file-loader": "5.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.1.0",
    "jest-serializer-vue": "2.0.2",
    "mini-css-extract-plugin": "0.9.0",
    "mutationobserver-shim": "0.3.3",
    "normalize.css": "8.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss": "7.0.27",
    "postcss-loader": "3.0.0",
    "rimraf": "3.0.2",
    "sass-loader": "8.0.2",
    "standard-version": "7.1.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "3.0.0",
    "vue-jest": "3.0.5",
    "vue-loader": "15.9.0",
    "vuepress": "1.3.1",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "html",
      "jest"
    ],
    "settings": {
      "html/html-extensions": [
        ".html"
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    }
  },
  "eslintIgnore": [
    "/dist"
  ]
}
