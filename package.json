{
  "name": "material-components-vue",
  "version": "1.0.0-alpha.0",
  "description": "Material Design components for Vue.js",
  "author": "Mats Pfeiffer <mats.pfeiffer@googlemail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matsp/material-components-vue.git"
  },
  "keywords": [
    "material-components-web",
    "mdc-web",
    "material-design",
    "vue"
  ],
  "homepage": "https://matsp.github.io/material-components-vue",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .js --ext .vue --fix ./",
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:min": "cross-env NODE_ENV=production webpack --config webpack.config.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:prod": "vuepress build docs",
    "watch:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js -w",
    "release": "standard-version",
    "test": "cross-env NODE_ENV=testing jest",
    "test:watch": "yarn test --watch",
    "test:updateSnapshot": "yarn test --updateSnapshot"
  },
  "dependencies": {
    "material-components-web": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vuepress": "^0.14.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "html",
      "jest"
    ],
    "settings": {
      "html/html-extensions": [
        ".html"
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    }
  },
  "eslintIgnore": [
    "/dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@material)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  }
}
