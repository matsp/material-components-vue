{
  "name": "material-components-vue",
  "version": "0.12.13",
  "description": "mdc-web integration for Vue.js",
  "author": "Mats Pfeiffer <mats.pfeiffer@googlemail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matsp/material-components-vue.git"
  },
  "homepage": "https://matsp.github.io/material-components-vue",
  "scripts": {
    "lint": "eslint --ext .js --ext .vue --fix ./",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build:prod": "webpack --config webpack.config.prod.js",
    "build:lib": "webpack --config webpack.config.lib.js",
    "build:es5": "webpack --config webpack.config.lib.es5.js",
    "build:all": "yarn build:lib && yarn build:es5 && yarn build:prod",
    "serve": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.dev.js",
    "release": "standard-version"
  },
  "dependencies": {
    "material-components-web": "^0.31.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-object-assign": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-stage-2": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.0",
    "core-js": "^2.5.3",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "standard-version": "^4.3.0",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.0.0",
    "webpack-merge": "^4.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "Firefox ESR",
    "not ie <= 10",
    "not ie_mob <= 10",
    "not android <= 4.4.3"
  ],
  "babel": {
    "comments": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      [
        "@babel/preset-stage-2"
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-object-assign"
    ]
  }
}
